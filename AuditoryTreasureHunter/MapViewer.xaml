<UserControl x:Class="LSRI.TreasureHunter.MapViewer"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="180" d:DesignWidth="340">
    
    <Grid x:Name="LayoutRoot" Background="White"
          MouseOver
          >
        <Viewbox Height="180" HorizontalAlignment="Left" Name="_viewBox" VerticalAlignment="Top" Width="340" Stretch="None">
            <Image Name="_image" Source="/AuditoryTreasureHunter;component/Media/map.jpg" Stretch="None" Margin="0" 
                   MouseMove="image1_MouseMove" 
                   MouseEnter="_image_MouseEnter" 
                   />
        </Viewbox>
       
    </Grid>
</UserControl>
